<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="lab.model.test.TestProfile" table="test_profile">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">sc_test_profile</param>
			</generator>
		</id>
		<property name="description" column="description"/>
		<bag name="testDescriptions" table="test_description_profile" lazy="false">
			<key column="test_profile"/>
			<many-to-many column="test_description" class="lab.model.test.TestDescription"/>
		</bag>
	</class>
</hibernate-mapping>