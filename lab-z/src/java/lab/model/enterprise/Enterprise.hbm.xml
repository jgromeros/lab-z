<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="lab.model.enterprise.Enterprise" table="enterprise">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">sc_enterprise</param>
			</generator>
		</id>
		<property name="identityNumber" column="dni"/>
		<property name="name" column="first_name"/>
		<property name="lastName" column="last_name"/>
		<property name="address" column="address"/>
		<property name="city" column="city"/>
		<property name="phone" column="phone"/>
		<property name="email" column="email"/>
		<property name="numeroICA" column="numero_ica"/>
	</class>
</hibernate-mapping>